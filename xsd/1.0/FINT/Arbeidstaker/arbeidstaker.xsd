  <xs:import namespace="https://ns.felleskomponent.no/felles/0.1" schemaLocation="fintfelles.xsd" />
  <xs:element name="Arbeidstaker" type="ar:ArbeidstakerType" />
  <xs:complexType name="ArbeidstakerListe">
    <xs:sequence>
      <xs:element name="arbeidstaker" type="ar:ArbeidstakerType" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ArbeidstakerType">
    <xs:annotation>
      <xs:documentation>Alle arbeidstakere i HR-systemet. 

Inneholder "ordinære" ansatte i fast og vikariat, i tillegg til oppdragstakere (politikere, ressurser med timelønn, sensorer, etc).

Inneholder ikke leverandørrepresentant (eksterne konsulenter etc).</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="fm:AbstraktPersonType">
        <xs:sequence>
          <xs:element name="signatur" minOccurs="1" maxOccurs="1" type="xs:string" />
          <xs:element name="utdanningsKodeId" minOccurs="1" maxOccurs="1" type="kv:BegrepType" />
          <xs:element name="tjenesteansienitet" minOccurs="1" maxOccurs="1" type="xs:date" />
          <xs:element name="fagforeningId" minOccurs="1" maxOccurs="1" type="xs:string" />
          <xs:element name="pensjonType" minOccurs="1" maxOccurs="1" type="xs:string" />
          <xs:element name="bankkonto" minOccurs="1" maxOccurs="1" type="xs:string" />
          <xs:element name="yrkesKodeId" minOccurs="1" maxOccurs="1" type="xs:string" />
          <xs:element name="stillinger" minOccurs="0" maxOccurs="1" nillable="true" type="ar:StillingListe" />
          <xs:element name="ansettelsesperiode" minOccurs="0" maxOccurs="1" nillable="true" type="fm:PeriodeType" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="Stilling" type="ar:StillingType" />
  <xs:complexType name="StillingListe">
    <xs:sequence>
      <xs:element name="stilling" type="ar:StillingType" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="StillingType">
    <xs:sequence>
      <xs:element name="avdeling" minOccurs="0" maxOccurs="1" nillable="true" type="xs:string" />
      <xs:element name="funksjon" minOccurs="0" maxOccurs="1" nillable="true" type="xs:string" />
      <xs:element name="grunnlonn" minOccurs="1" maxOccurs="1" type="xs:double" />
      <xs:element name="organisasjonId" minOccurs="0" maxOccurs="1" nillable="true" type="xs:string" />
      <xs:element name="statusId" minOccurs="1" maxOccurs="1" type="xs:string" />
